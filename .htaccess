DirectoryIndex index.php

RewriteEngine On
RewriteRule ^([a-zA-Z0-9_\-]*)?/?$ index.php?page=$1&%{QUERY_STRING}

ErrorDocument 404 /index.php?page=error

DirectoryIndex indexAdmin.php

AuthName "Cette espace est resever a ladministrateur du site ceci est un contenu proteger"
AuthType Basic
AuthUserFile "C:\xampp\htdocs\www\mvc\controllers\admin\.htpasswd"
Require valid-user

RewriteRule ^([a-zA-Z0-9_\-]*)?/?$ indexAdmin.php?admin=$1&%{QUERY_STRING}
